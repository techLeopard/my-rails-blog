<%= form_with model: ulink do |form| %>
    <div>
      <%= form.label :title %><br>
      <%= form.text_field :title %>
      <% ulink.errors.full_messages_for(:title).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
  
    <div>
      <%= form.label :body %><br>
      <%= form.url_field :body %><br>
      <% ulink.errors.full_messages_for(:body).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
  
    <div>
      <%= form.submit %>
    </div>
    
<% end %> 