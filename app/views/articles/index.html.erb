<% provide(:title, "Articles") %>

<h1>Articles</h1>

<ul class = "list-group" style="list-style: none;">
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article %>
    </li>
  <% end %>
</ul>
<hr>
<%= link_to "New Article", new_article_path, class: "btn btn-primary" %>
